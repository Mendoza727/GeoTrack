{
  "expo": {
    "name": "GeoTrack",
    "slug": "GeoTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/GeoTrack-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/GeoTrack-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the location to track your location",
        "NSLocationAlwaysUsageDescription": "This app uses the location to track your location",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses the location to track your location"
      },
      "bundleIdentifier": "com.geotrack.app",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_API_KEY_IOS"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/GeoTrack-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.geotrack.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_API_KEY_ANDROID"
        }
      }
    },
    "web": {
      "favicon": "./assets/GeoTrack-icon.png"
    }
  }
}
